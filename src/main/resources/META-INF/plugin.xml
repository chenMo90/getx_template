<idea-plugin>
    <id>com.tao.getx</id>
    <!-- Plug-in name, the name displayed by the official plugin library -->
    <name>GetX</name>
    <!-- version number  -->
    <!--    <version>1.1</version>-->
    <!-- homepage and email (contact address)-->
    <vendor email="xdd666@foxmail.com" url="https://github.com/CNAD666/getx_template">小呆呆666</vendor>

    <!-- The description of the plug-in
    (the default value cannot be used, it must be modified to its own.
    And it needs to be greater than 40 characters)-->
    <description><![CDATA[
    <h2>Used to generate the template code of GetX framework</h2>
    <ul>
        <li>Right-click the File: New -> GetX</li>
        <li>GitHub：<a href="https://github.com/CNAD666/getx_template">getx_template</a></li>
        <h3>GetX Use Article</h3>
        <li>English：<a href="https://github.com/CNAD666/getx_template/blob/main/docs/Use%20of%20Flutter%20GetX---simple%20charm!.md"> Flutter GetX Use --- simple charm!</a></li>
        <li>掘金：<a href="https://juejin.cn/post/6924104248275763208"> Flutter GetX使用---简洁的魅力！</a></li>
    </ul>
    ]]></description>


    <!-- Plug-in version change information Support HTML tags-->
    <!--    <change-notes><![CDATA[-->
    <!--    <h1>1.1</h1>-->
    <!--    <ul>-->
    <!--        <li>You can generate a large number of GetX template codes</li>-->
    <!--        <li>Improve development efficiency</li>-->
    <!--        <li>If you have any questions, please give feedback</li>-->
    <!--    </ul>-->
    <!--    ]]></change-notes>-->

    <!--  It is recommended to write the id of other plug-ins that the plug-in depends on. Do
    not write it. In some cases, uploading the plug-in will report an error -->
    <depends>com.intellij.modules.all</depends>

    <!-- Plug-in compatible with IDEA Build number-->
    <idea-version since-build="173"/>
    <!--    <idea-version since-build="173" until-build="203.7148.57"/>-->

    <!-- Write plugin actions -->
    <actions>
        <!-- Add your actions here -->
        <action id="com.tao.NewGetX" class="NewGetX" text="GetX" description="create GetX template code"
                icon="/image/icon.jpg">
            <add-to-group group-id="NewGroup" anchor="first"/>
        </action>
    </actions>

    <extensions defaultExtensionNs="com.intellij">
        <!-- application level-->
        <applicationService serviceImplementation="helper.DataService"/>
        <applicationConfigurable parentId="tools" instance="setting.SettingsConfigurable"
                                 id="SettingsConfigurable" displayName="GetX Setting"/>
        <!--Action-->
        <intentionAction>
            <className>action.WrapWithObxAction</className>
            <category>GetX</category>
        </intentionAction>
    </extensions>
</idea-plugin>